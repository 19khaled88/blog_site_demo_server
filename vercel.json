{
    "version":2,
    "name": "backend",
    "builds":[
        {
            "src":"build/index.js",
            "use":"@vercel/node"
            
        }
    ],
    "routes":[
        {
            "src":"/(.*)",
            "dest":"build/index.js",
            "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
			"headers": {
				"Access-Control-Allow-Origin": "*"
			}
        }
    ]
}